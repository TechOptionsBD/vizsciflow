{
    "name":"galaxy_101_upload_2",
    "desc":"MyExperiment Sample with some modification",
    "script":[
        "data = galaxy.Upload('/public/paeruginosa-reads/SRR396636.sra_1.fastq')",
        "data2 = galaxy.Upload('/public/paeruginosa-reads/SRR396636.sra_2.fastq')",
        "",
        "data = galaxy.FastqToSam(data)",
        "data2 = galaxy.FastqToSam(data2)",
        "datatype = galaxy.GetDataType(data)",
        "if datatype == 'galaxy.datatypes.binary.Bam':",
        "    data = galaxy.BamToSam(data)",
        "",
        "datatype = galaxy.GetDataType(data2)",
        "if datatype == 'galaxy.datatypes.binary.Bam':",
        "    data2 = galaxy.BamToSam(data2)",
        "    ",
        "data = galaxy.SamToInterval(data)",
        "data2 = galaxy.SamToInterval(data2)",
        "",
        "data = galaxy.iJoin(data, data2, min=1)",
        "data = galaxy.Group(data, groupcol=1) # op1='mean|1|no' optype=mean|median|mode|max|min|sum|length|unique|cat|cat_uniq|randomly|std",
        "data = galaxy.Sort(data, col='1|num|DESC') # style=num|gennum|alpha, order=DESC|ASC",
        "selected_data = galaxy.SelectFirst(data, lines=10)",
        "data = galaxy.Compare(data, selected_data, field1=1, field2=1)",
        "data = galaxy.Download(data, '/public')",
        "print(data)"
    ]
}