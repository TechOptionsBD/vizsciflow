{
    "name":"Galaxy: Merge-Align-Convert",
    "desc":"Alignment and merge operation done in galaxy. Data uploaded to galaxy from local machine for each operation",
    "script":[
        "fq_html = galaxy.CheckQuality('/public/MiSeq_SOP/F3D7_S195_L001_R1_001.fastq')",
        "print(fq_html)",
        "fq_html = galaxy.CheckQuality('/public/MiSeq_SOP/F3D7_S195_L001_R2_001.fastq')",
        "print(fq_html)",
        "",
        "# merge these files",
        "data = galaxy.Merge('/public/MiSeq_SOP/F3D7_S195_L001_R1_001.fastq', '/public/MiSeq_SOP/F3D7_S195_L001_R2_001.fastq')",
        "print(data)",
        "",
        "# align these files with a reference dna",
        "data = galaxy.Align('/public/Chr1.cdna', '/public/MiSeq_SOP/F3D7_S195_L001_R1_001.fastq', '/public/MiSeq_SOP/F3D7_S195_L001_R2_001.fastq')",
        "print(data)",
        "",
        "# convert the aligned sam file to bam format",
        "data = galaxy.SamToBam('/public/Chr1.cdna', data)",
        "print(data)",
        ""
    ]
}