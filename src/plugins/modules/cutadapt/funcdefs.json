{
    "package":"cutadapt",
    "name":"CutAdapt",
    "internal":"run_cutadapt",
    "org":"srlab",
    "group":"Quality",
    "desc":"Finds and removes adapter sequences, primers, poly-A tails and other types of unwanted sequence from high-throughput sequencing reads",
    "href":"",
    "params":[
       {
          "name":"data",
          "type":"file",
          "desc":"",
          "default":""
       },
       {
        "name":"data2",
        "type":"file",
        "desc":"",
        "default":"''"
       },
       {
         "name":"adapter",
         "type":"str",
         "desc":"",
         "default":"'CTGTCTCTTATACACATCT'"
       },
       {
          "name":"min_length",
          "type":"int",
          "desc":"Discard reads shorter than LEN",
          "default":"0"
       },
       {
           "name":"quality",
           "type":"int",
           "desc":"Trim low-quality bases from 5' and/or 3' ends of each read before adapter removal.",
           "default":"20"
       }
    ],
    "returns":[
       {
          "name":"output",
          "type":"file[]"
       }
    ]
 }